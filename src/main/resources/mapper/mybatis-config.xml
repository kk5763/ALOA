<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aloa.dao.MemberMapper">

    <select id="inspectEmail" parameterType="java.lang.String" resultType="com.aloa.dto.MemberVO">
        SELECT * FROM aloa_user WHERE email=#{email}
    </select>
    
    <insert id="resInsert" parameterType="com.aloa.restaurant.Restaurant" >
    	insert into restaurant values(seq_restaurant.nextval,
    		#{resname},
    		#{bossemail},
    		#{resaddress},
    		#{restel},
    		#{reskind},
    		#{resprice},
    		#{resparking},
    		#{ressaletime},
    		#{resreset},
    		#{resholiday},
    		#{request},
    		sysdate,
    	)
    </insert>
 
</mapper>